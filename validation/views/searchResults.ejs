<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <h1><%= title %></h1>
        <% if (locals.user !== null) { %>
            <p>
                ID: <%= user.id %>, Name: <%= user.firstName %> <%= user.lastName %>
                Email: <%= user.email%> Age: <%= user.age%> Bio: <%= user.bio%>
            </p>
        <% } else { %>
            <p> No users found </p>
        <% } %>
        <a href="/">Back to home</a>
    </body>
</html>

